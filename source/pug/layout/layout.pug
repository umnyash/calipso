block variables
block data

doctype html
html.page(class= pageClass lang='ru')
  head
    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    link(rel='preload' href='fonts/mulish-400.woff2' as='font' type='font/woff2' crossorigin)
    link(rel='preload' href='fonts/mulish-500.woff2' as='font' type='font/woff2' crossorigin)
    link(rel='preload' href='fonts/mulish-600.woff2' as='font' type='font/woff2' crossorigin)
    link(rel='stylesheet' href='css/style.min.css')
    title= title
    meta(name='description' content='Интерьерный салон «Калипсо» — богатый выбор и безупречное качество.')
    meta(name='keywords' content='Калипсо, интерьер, мебель.')
  body.page__body
    .page__inner
      main.page__content
        block content
    script(src='js/script.min.js')
